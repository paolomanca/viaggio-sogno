<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Viaggio-Sogno-EJB">
		<jta-data-source>jdbc/travelDreamConnection</jta-data-source>
		<class>model.FinalExcursion</class>
		<class>model.FinalFlight</class>
		<class>model.FinalHotel</class>
		<class>model.FinalPackage</class>
		<class>model.IdPool</class>
		<class>model.Package</class>
		<class>model.PackageHasProduct</class>
		<class>model.PackageHasProductPK</class>
		<class>model.Product</class>
		<class>model.User</class>
		<class>model.FinalProduct</class>
		<properties>
			<property name="eclipselink.ddl-generation" value="drop-and-create-tables"/>
			<property name="eclipselink.logging.level" value="FINE"/>
		</properties>
	</persistence-unit>
</persistence>
